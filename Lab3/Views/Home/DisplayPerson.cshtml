@{
    Layout = "_Layout";
    ViewBag.Title = "Display Person";
}

<h3>Your favorite Person</h3>
<table class= "center2"> 
    <tr>
        <td>
            First Name:
        </td>
        <td>
            @Model.FirstName
        </td>
    </tr>
    <tr>
        <td>
            Last Name:
        </td>
        <td>
            
            @Model.LastName
        </td>
    </tr>
    <tr>
        <td>
            Age:
        </td>
        <td>
            @Model.Age
        </td>
    </tr>
    <tr>
        <td>
            Email:
        </td>
        <td>
            @Model.EmailAddresss 
        </td>
    </tr>
    <tr>
        <td>
            Description:
        </td>
        <td>
            @Model.Desc
        </td>
    </tr>
</table>
